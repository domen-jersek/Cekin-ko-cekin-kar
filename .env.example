# Discord
DISCORD_TOKEN=
CLIENT_ID=
GUILD_ID=

# Google Sheets
SPREADSHEET_ID=
SHEET_NAME=cekincki
# Absolute path to your Google service account JSON credentials
GCP_CREDENTIALS_PATH=c:\\path\\to\\your\\service-account.json

# Role tiers (JSON). Highest tier with min <= value is assigned.
# Example default is used if unset.
# CEKINCKI_TIERS=[{"min":0,"name":"Novice"},{"min":10,"name":"Bronze"},{"min":50,"name":"Silver"},{"min":100,"name":"Gold"},{"min":500,"name":"Platinum"}]

# Role name prefix
CEKINCKI_ROLE_PREFIX=cekinčki

# Auto sync interval in seconds (default 300 = 5 minutes)
SYNC_INTERVAL_SECONDS=300

# Notifications when cekinčki value changes
# true | false
NOTIFY_ON_CHANGE=true
# dm | channel | both
NOTIFY_MODE=dm
# Channel ID used when NOTIFY_MODE is channel or both
NOTIFY_CHANNEL_ID=
# Message template tokens: {nickname} {old} {new} {guild} {userMention}
NOTIFY_TEMPLATE=Your cekinčki changed from {old} to {new}.
